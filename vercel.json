{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/color_changing_nft_frontend/build/index.html"
    }
  ],
  "buildCommand": "cd color_changing_nft_frontend && npm install && npm run build",
  "outputDirectory": "color_changing_nft_frontend/build",
  "github": {
    "silent": true
  }
}
